class Solution {
public:
    bool isOneBitCharacter(vector<int>& bits) {
        int n = bits.size();
        int k=0;
        for(int i=0;i<n;i++){
            if(bits[i]==1 and k<=0)k=1;
            else if(k==1){
                k=0;
            }else k=-1;
        }
        if(k==-1) return true;
        return false;
    }
};
