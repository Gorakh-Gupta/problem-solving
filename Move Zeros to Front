class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        if (nums.size() < 2)
            return;
        int i = 0, j = 0;
        int n = nums.size();
        while (i < n) {
            if (nums[i] == 0) {
                if (nums[j])
                    swap(nums[i], nums[j]);
                i++;
                j++;
            } else {
                i++;
            }
        }
        return;
    }
};
