class Solution {
public:
    bool f(string s) {
        int n = s.size();
        if (n == 0)
            return false;
        for (int i = 0; i < n; i++) {
            if (isalnum(s[i]) || s[i] == '_')
                continue;
            else
                return false;
        }
        return true;
    }

    bool ff(string s) {
        vector<string> l = {"electronics", "grocery", "pharmacy", "restaurant"};
        for (int i = 0; i < 4; i++) {
            if (s == l[i])
                return true;
        }
        return false;
    }

    vector<string> validateCoupons(vector<string>& code, vector<string>& a,
                                   vector<bool>& isActive) {

        int n = code.size();
        vector<pair<string, string>> ans;
        for (int i = 0; i < n; i++) {
            if (f(code[i]) and ff(a[i]) and isActive[i])
                ans.push_back({a[i], code[i]});
        }

        sort(ans.begin(), ans.end());

        vector<string> res;
        for (auto x : ans){
        // cout<<x.first<<" "<<x.second<<endl;
            res.push_back(x.second);}
        return res;
    }
};
