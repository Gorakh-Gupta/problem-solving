class Solution {
public:
    vector<int> minBitwiseArray(vector<int>& nums) {
        vector<int> v;
        for (auto x : nums) {
            int k = 0;
            for (int i = 1; i < x; i++) {
                if ((i | (i + 1)) == x) {
                    v.push_back(i);
                    k = 1;
                    break;
                }
            }
            if (k == 0)
                v.push_back(-1);
        }
        return v;
    }
};
