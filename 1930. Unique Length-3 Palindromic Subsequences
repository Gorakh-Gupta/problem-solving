class Solution {
public:
    int countPalindromicSubsequence(string s) {

        map<char, pair<int,int>>mp;
        int n = s.size();
       
        for(int i=0;i<n;i++){
            if(mp[s[i]].first==0)
            mp[s[i]].first = i+1;
        }

         for(int i=n-1;i>-1;i--){
            if(mp[s[i]].second==0)
            mp[s[i]].second = i+1;
        }
        int ans =0;

        for(auto x:mp){
            // cout<<x.second.first<<" "<<x.second.second;
            unordered_set<int>ss;
            for(int i=x.second.first;i<x.second.second-1;i++){
             ss.insert(s[i]);
            }
            ans+=ss.size();

        }
        return ans;
        
        
    }
};
