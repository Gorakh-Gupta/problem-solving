#include<bits/stdc++.h>
using namespace std;
void solve();

  bool fun(int m){
 int h=0;
  while(m>0){
              int r = m%10;
              if(r > 1){h=1;break;}
              m/=10;
  }
  if(h==0)return true;
  return false;
  }

int main(){
    int t;
    cin>>t;
    while(t--){
    int k,n;
              cin>>n>>k;
              set<int>s;
              int x, ans =0;
              vector<int>v(n);
              for(int i=0;i<n;i++)cin>>v[i];
                     for(int i=0;i<k;i++){
                                 cin>>x;
                                 cout<<min(v[0]-1,x)<<" ";
}
cout<<endl;
    }
}
