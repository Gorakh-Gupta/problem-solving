#include<bits/stdc++.h>
using namespace std;
void solve();


int main(){
    int t;
    cin>>t;
    while(t--){
    int n, k, q;
    cin>>n;
    n--;
   vector<int>v(n);int j =0;
   for(int i=0;i<n;i++)cin>>v[i];
   v[0]+=503;
   for(int i=1;i<=n;i++){
    v[i]+=v[i-1];
   }
   cout<<503<<" ";
   for(auto x:v)cout<<x<<" ";
    cout << endl;}}
 
