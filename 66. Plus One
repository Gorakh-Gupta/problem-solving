class Solution {
public:
    vector<int> plusOne(vector<int>& a) {
        vector<int> ans;
        int cur = 1;
        for (int i = a.size() - 1; i >= 0; i--) {
            int h = a[i];
            h += cur;
            ans.push_back(h % 10);
            cur = h / 10;
        }
        if(cur>0)ans.push_back(cur);
        reverse(ans.begin(), ans.end());
        return ans;
    }
};
