#include<bits/stdc++.h>
using namespace std;
void solve();


int main(){
    int t;
    cin>>t;
    while(t--){
    int n, k, q;
   int a,b,c,d;
   cin>>a>>b>>c>>d;
   if(a>b){
   int t;
   t=a;
   a=b;
   b=t;}
   if(c>d){
   int t;
   t=c;
   c=d;
   d=t;}
    if((((c>=a and c<=b) && (d>=a and d<=b)) || ((a>=c and a<=d) && (b>=c and b<=d))) || 
   (((c>=a and c>=b) && (d>=a and d>=b)) || ((a>=c and a>=d) && (b>=c and b>=d))))cout<<"NO";
   else cout<<"YES";
    cout << endl;}}
