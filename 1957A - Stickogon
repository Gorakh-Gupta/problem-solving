#include<bits/stdc++.h>
using namespace std;
void solve();

  bool fun(int m){
 int h=0;
  while(m>0){
              int r = m%10;
              if(r > 1){h=1;break;}
              m/=10;
  }
  if(h==0)return true;
  return false;
  }

int main(){
    int t;
    cin>>t;
    while(t--){
    int k,n,d,c;
              cin>>n;
            //   vector<int>ans;
              map<int,int>mp;
              int ans = 0;
              for(int i=0;i<n;i++){
                         cin>>k;
                         mp[k]++;
              }for(auto x:mp){
                          if(x.second > 2)ans+=x.second/3;
              }cout<<ans<<endl;
    }}
    
            
