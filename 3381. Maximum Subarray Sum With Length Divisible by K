class Solution {
public:
    long long maxSubarraySum(vector<int>& nums, int k) {
        vector<long long> prefix(k, LONG_LONG_MAX / 2);
        long long s = 0, ans = LONG_LONG_MIN;
        int n = nums.size();
        prefix[k - 1] = 0;
        for (int i = 0; i < n; i++) {
            s += nums[i];
            ans = max(ans, s - prefix[i % k]);
            prefix[i % k] = min(prefix[i % k], s);
        }

        return ans;
    }
};
