#include<bits/stdc++.h>
using namespace std;
void solve();

int main(){
    int t;
    cin>>t;
    while(t--){
    long long int n,d,p,c;
              cin>>n;
              string s;
              cin>>s;
              p=0;
              for(auto x:s){
                 if(x=='U')p++;
              }
              if(p&1)cout<<"Yes";
              else cout<<"No";
              cout<<endl;
    }}
